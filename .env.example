# LLMプロバイダー設定 (openrouter または ollama)
LLM_PROVIDER=openrouter

# OpenRouter設定
OPENROUTER_API_KEY=sk-or-v1-xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
OPENROUTER_MODEL_ID=meta-llama/llama-3.1-8b-instruct

# 実行設定
MAX_CONCURRENT_REQUESTS=3
REQUEST_TIMEOUT_MS=30000
RETRY_COUNT=3

# データベース設定
DB_PATH=./data/companies.db

# 出力設定
OUTPUT_DIR=./output
LOG_LEVEL=info